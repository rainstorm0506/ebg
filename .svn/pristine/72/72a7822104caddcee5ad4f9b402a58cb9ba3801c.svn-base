	<!---------- header ---------->
	<header-public></header-public>
	
	<!---------- main ---------->
	<main>
		<!---- 图片 ---->
		<section class="goods-detail-pic">
			<img ng-src="{{data.cover}}">
			<coll-cart></coll-cart>
		</section>
		<!---- 商品信息 ---->
		<section class="box-wrap-info">
			<h3 class="tit-second" ng-bind="data.title"></h3>
			<p class="txt-c"><span>转让价：</span><em>￥<x ng-bind="data.sale_price"></x></em></p>
			<p class="txt-d"><span>原价：</span><del>￥<x ng-bind="data.buy_price"></x></del><em>（购买前请联系卖家）</em></p>
			<p class="txt-e"><span>所在地区：</span><em>成都市青羊区</em></p>
			<p><span>成色：</span><em><x ng-bind="data.use_time"></x>新</em></p>
			<div class="h10px"></div>
			<!--
			<ul class="service-list">
				<li><aside>产品规格及数量</aside><s class="tr-r"><i></i><b></b></s><a href="#"></a></li>
			</ul>
			-->
		</section>
		<div class="h10px"></div>
		<!---- 商品详情 ---->
		<section class="box-wrap-detail">
			<header class="box-tit box-tit-1">
				<figure><img ng-src="{{imgPath}}/temp/11.png"></figure>
				<div>
					<h6>东华数码专营店</h6>
					<div class="star-wrap">
						<i class="current"></i>
						<i class="current"></i>
						<i class="current"></i>
						<i></i>
						<i></i>
					</div>
				</div>
				<s class="tr-r"><i></i><b></b></s>
				<a href="#"></a>
			</header>
			<section class="owner-info">
				<div>
					<p>王大锤</p>
					<h6>店主</h6>
				</div>
				<div>
					<p>20年</p>
					<h6>经营年限</h6>
				</div>
				<div>
					<p>10000</p>
					<h6>保证金</h6>
				</div>
			</section>
			<section class="site-wrap">
				<p>地址：东华电脑城23-4-1</p>
				<i></i>
			</section>
			<div class="drag-txt">
				<i></i>
				<p>继续拖动查看图文详情</p>
				<i></i>
			</div>
			<div class="editor-wrap" ng-bind-html="data.content">
				<img ng-src="{{imgPath}}/temp/12.png">
			</div>
