a{color:#00F}
#attrs , #attrs table,.show-price , #goodsImgGruoup{display:none}
#goodsAttrs .txt *{float:none}
#attrs #goodsAttrs .txt span{background-color:#FFF;color:#00F}
#attrs #goodsAttrs .txt span label{color:#000}
#goodsAttrs table td.this{color:#FFF;background-color:#000;cursor:pointer}
.form-list-34 form aside{width:1000px}
.form-list-34 select.error{margin-left:0;color:#000}
.form-list-34 select.mlr10px{margin-left:10px}
.form-list-34 .per-wrap{border-bottom:1px dotted #CCC;padding:0 20px 10px 0;width:auto}
.form-list-34 .per-wrap .js-drag{cursor:pointer}
.form-list-34 .per-wrap strong{width:193px;text-align:right;padding:0 10px 0 0}
.form-list-34 .per-wrap strong b{font-weight:400;color:#999;margin:0 10px 0 0}
.form-list-34 .per-wrap .tbox34-3{text-align:center}
span.errorMessage{margin:0 0 0 12px}
.goods-property > div{border-top:1px solid #CCC;padding:10px 0 0 0}
.goods-property > div.txt{height:auto;padding:0;color:#000;margin-left:0}
#attrs .goods-property > div span{line-height:18px;background-color:#000;color:#FFF;padding:1px 5px 3px;margin-right:0}
.infinite-stock input{vertical-align:top}
.infinite-stock a{display:inline-block;border:1px solid #CCC;border-radius:5px;height:30px;line-height:30px;padding:0 10px;margin:0 10px 0 0;color:#888}
.infinite-stock a.this{border-color:#00B266;background-color:#00B266;color:#FFF}
li.infinite-stock a{height:34px;line-height:34px;margin:0 0 0 5px}
#goodsDefaultSet input{width:137px;margin:0 5px 0 0}
th.show-price{color:#c00}
span.show-price{color:#00F}
span.show-price b{font-weight:400;margin:0 5px 0 15px;color:#666}

.goods-args ul{border:1px solid #CCC;padding:20px;margin:0 0 20px 0;width:590px}
.goods-args span{width:100px;text-align:right}
.goods-args a{display:inline-block;border:1px solid #999;border-radius:5px;padding:0 12px;background-color:#EEE;color:#333}
.goods-args a:hover{background-color:#999;color:#FFF}
.goods-args a.args-dels{border:0 none;background-color:#FFF;color:#00F}
.goods-args b{font-weight:400;margin:0 0 0 20px;color:#333}
.goods-args .heads{border-bottom:1px dotted #CCC;padding:0 0 10px 0}
.goods-args .heads input{width:426px}
.goods-args input.tbox-left{width:170px}
.goods-args input.tbox-right{width:196px}
.goods-args input.error{color:#F00}
.goods-args .errorMessage{padding:0 0 0 100px}
.goods-args .args-delete{margin:0 0 0 234px}
.goods-args .args-amenity{margin:0 0 0 20px}

.pack-more{color:#00F}
.goods-img .webuploader-container{float:left;position:relative;z-index:0;text-align:center;border:1px solid #ccc;height:120px;width:120px;margin-right:20px}
.goods-img .webuploader-container input{left:0;position:absolute;top:0;z-index:0;background-color:#FFF;opacity:0;height:100%;width:100%}
.goods-img .webuploader-container i{color:#e1e1e1;display:block;font-size:60px;margin-top:10px;height:60px;line-height:60px;font-style:normal}
.goods-img .webuploader-container p{color:#999;top:-10px}
.goods-img .webuploader-container img{position:absolute;top:0;left:0;width:100%;height:100%;z-index:1;margin:0}
.goods-img .webuploader-container b{position:absolute;top:55px;left:0;z-index:2;width:100%;font-weight:400; text-align:center}
.goods-img .webuploader-container b.uploading{top:0;z-index:99;height:100%;background-color:#FFF;display:block;line-height:120px}
.goods-img a.preview-close{top:0;right:0;line-height:20px;padding:0 5px;font-size:16px;position:absolute;z-index:2;display:inline-block;cursor:pointer}
.goods-img .preview-set *{position:absolute;bottom:0;left:0;height:30px;line-height:30px;display:block;width:100%}
.goods-img .preview-set span{z-index:998;background-color:#000;opacity:0.6;filter:alpha(opacity=60);}
.goods-img .preview-set a{z-index:999;color:#FFF}
.goods-img .preview-set a.this{color:#0F0}
label.attrs-img{margin:0 0 0 5px;color:#00F}
#goodsImgGruoup .text-line{margin:-20px 0 0 0;padding:0 0 0 110px;color:#00F}
.user-merchant{width:40.5%;clear:both;margin:10px 0 0 110px;border-collapse:collapse;}
.user-merchant th{background-color:#FF9900;text-align:center;font-weight:400;color:#FFF;border:1px solid #FFF}
.user-merchant tr.this td{background-color:#ABCDEF}
.user-merchant td{text-align:center;background-color:#CCFF99;border:1px solid #FFF}
.user-merchant ._l{text-align:left;padding:0 0 0 5px}
.user-merchant .user-seek{cursor:pointer}
.user-merchant .user-seek.this{color:#00F}
.user-merchant td input{text-align:center;float:none}
.user-merchant td.txt{background-color:#FCFCFC}

.form-list-34 form h6{width:120px}

.goods-check a{margin:0 20px 0 0;background-color:#efefef;display:inline-block;padding:0 20px;border:1px solid #CCC;border-radius:5px}
.goods-check a:hover{background-color:#999;border:1px solid #333;color:#FFF}

.goods-tags span{margin:10px 0 0 0}
.goods-tags span input{margin:3px 0 0 0}