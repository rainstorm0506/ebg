	<!---------- header ---------->
	<header-public></header-public>
	<!---------- main ---------->
	<main>
		<ul class="my-commit-list">
			<li ng-repeat="$p in data">
				<figure><img ng-src="{{$p.goods_cover}}"></figure>
				<section>
					<h3 ng-bind="$p.goods_title"></h3>
					<article ng-bind="$p.content"></article>
					<div><span>规格：{{$p.attrs}}</span><time ng-bind="$p.goods_vers_num | date:'yyyy-mm-dd'"></time></div>
					<dl>
						<dd><img ng-src="{{$p.goods_cover}}"></dd>
					</dl>
				</section>
			</li>
		</ul>
		<no-data></no-data>
	</main>
	<a class="btn-1" ui-sref="order.commit">去评价</a>
	<promt></promt>