	<!---------- header ---------->
	<header-public></header-public>
	<!---------- main ---------->
	<main>
		<ul class="goods-list goods-list-a">
			<li ng-repeat="$p in data">
				<figure><a href=""><img ng-src="{{$p.cover}}"></a></figure>
				<h6><a href="#" ng-bind="$p.title"></a></h6>
				<p><span ng-bind="$p.base_price"></span><em><x ng-bind="$p.collect"></x>人收藏</em></p>
				<checkboxs ng-if="isEditor"></checkboxs>
			</li>
		</ul>
		<no-data></no-data>
		<div class="h40px"></div>
	</main>
	<nav class="btn-wrap"><a class="btn-1" ng-class="{disabled:disSubmit}" ng-click="editor()" href="javascript:;" ng-bind="editorTxt"></a></nav>
